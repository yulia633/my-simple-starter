{
    "name": "yulia633/simple-starter-php",
    "description": "My simple starter for project php",
    "type": "library",
    "authors": [
        {
            "name": "yulia633,",
            "email": "ignat633yulia@yandex.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Php\\Project\\": "src",
            "Php\\Project\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpcs": "phpcs",
        "phpunit": "phpunit"
    },
